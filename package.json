{
  "name": "@shopify/shopify-express",
  "version": "1.0.0-alpha.8",
  "author": "Shopify Inc.",
  "description": "Get up and running quickly with Express.js and the Shopify API.",
  "keywords": [
    "shopify",
    "express.js",
    "express middleware"
  ],
  "bugs": "https://github.com/Shopify/shopify-express/issues",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Shopify/shopify-express.git"
  },
  "engines": {
    "node": ">=12.13.0"
  },
  "scripts": {
    "prettier:check": "prettier-check --single-quote --trailing-comma=all {middleware,routes,shopStore}{/*,/**/*}.{js}",
    "prettier:fix": "prettier --single-quote --trailing-comma=all --write {middleware,routes,shopStore}{/*,/**/*}.{js}",
    "precommit": "lint-staged",
    "test": "jest",
    "test:ci": "jest --ci"
  },
  "lint-staged": {
    "*.{js}": [
      "prettier:check"
    ]
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "connect-redis": "^4.0.4",
    "cookie-parser": "^1.4.4",
    "express": "^4.17.1",
    "express-session": "^1.17.0",
    "knex": "^0.20.10",
    "node-fetch": "^2.6.0",
    "prop-types": "^15.7.2",
    "raw-body": "^2.4.1",
    "sqlite3": "^4.1.1",
    "url": "^0.11.0"
  },
  "devDependencies": {
    "find-free-port": "^2.0.0",
    "husky": "^4.2.3",
    "jest": "^25.1.0",
    "lint-staged": "^10.0.8",
    "prettier": "^1.19.1",
    "prettier-check": "^2.0.0"
  }
}
